		a.mostrarPag();
		a.pasarPagina();
		a.mostrarPag();
		a.pasarPagina();
		a.mostrarPag();
		a.pasarPagina();
		a.mostrarPag();
		a.pasarPagina();
		a.mostrarPag();
		a.pasarPagina();
		a.mostrarPag();
		a.pasarPagina();
		a.mostrarPag();
		b=new MarcaPagina(80);
		b.mostrarPag();
		b.empezarInicio();
		b.mostrarPag();
	}

