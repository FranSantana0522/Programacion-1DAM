package pack;

import java.util.Scanner;

public class Actividad_cuadrado {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int n;
		System.out.println("Introduzca numero : ");
		n=sc.nextInt();
		for(int i=0;i<=n;i++) {
			System.out.print(" * ");
		}
		System.out.println();
		
		for (int j=0;j<=n;j++) {
			System.out.println(" * ");
		}
		System.out.println(" ");
	}

}
