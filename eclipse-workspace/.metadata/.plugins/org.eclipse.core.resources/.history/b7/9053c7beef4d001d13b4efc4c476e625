package pack;

import java.util.Scanner;

public class ActividadAmpliacion3_15 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int n;
		int m;
		System.out.println("Introduzca un numer: ");
		n=sc.nextInt();
		for( n=1;n<=n;n++) {
			for( m=n;m<=n;m++) {
				System.out.print("* ");
			}
			System.out.println("");
		}
	}

}
