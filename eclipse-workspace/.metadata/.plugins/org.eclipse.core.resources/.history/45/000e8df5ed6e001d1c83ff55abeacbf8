		for (int i = 0; i < longi; i++) {
			v[i] = (int) (Math.random() * 20);
		}
		for (int i = 0; i < 2; i++) {
			System.out.println("Introduce dos valores: ");
				v[i]=sc.nextInt();
				
			}
